                <listApp>
                    <app>
                        <note xml:id="Ezek-025-fn1" type="footnote">ADub.III.83:
                                <emph>ʒyɣyrdqanyn</emph>; a scribal error.</note>
                        <note xml:id="Ezek-025-fn2" type="footnote">ADub.III.83: a redundant
                            repetition.</note>
                    </app>
