                    <app>
                        <note xml:id="Ezek-008-fn1" type="footnote">ADub.III.83:
                                <emph>kinilettivcinin</emph>; a scribal error.</note>
                        <note xml:id="Ezek-008-fn2" type="footnote">ADub.III.83: <emph>ajty</emph>;
                            a scribal error.</note>
                        <note xml:id="Ezek-008-fn3" type="footnote">ADub.III.83:
                                <emph>qussquncluluqnun</emph>; a scribal error.</note>
                        <note xml:id="Ezek-008-fn4" type="footnote">ADub.III.83: a redundant full
                            stop.</note>
                    </app>
                </listApp>
