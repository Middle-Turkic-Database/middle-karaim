                    <app>
                        <note xml:id="Chron2-002-fn1" type="footnote">In Hebrew Bible: <foreign
                                xml:lang="he" style="unicode-bidi: embed;">והבית</foreign>
                            .</note>
                        <note xml:id="Chron2-002-fn2" type="footnote">TKow.02: scribal error
                                <foreign xml:lang="he" style="unicode-bidi: embed;"
                                >כֶיכְקְלֶיר</foreign>
                            .</note>
                        <note xml:id="Chron2-002-fn3" type="footnote">TKow.02: scribal error
                                <foreign xml:lang="he" style="unicode-bidi: embed;"
                                >כֶיכְקְלֶיר</foreign>
                            .</note>
                        <note xml:id="Chron2-002-fn4" type="footnote">Cf. with the Russian word
                                <emph>сплав</emph>.</note>
                    </app>
