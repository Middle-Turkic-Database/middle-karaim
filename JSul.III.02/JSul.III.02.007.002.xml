    <text>
        <body>
            <div type="edition">
                <div type="textpart" subtype="chapter">
                    <ab>
                        <title>
                            <supplied reason="lost">Judges 2</supplied>
                        </title>
                        <milestone unit="page" n="37r"/>
                        <milestone unit="line" n="1"/>
                        <l n="14" xml:id="Judg-002-014">da bolaj almadylar daġyn turma aldyŋa dušmanlarynyŋ.<milestone unit="line" n="2"/></l>
                        <l n="15" xml:id="Judg-002-015">
                            <foreign xml:lang="he" style="unicode-bidi: embed;">בכל</foreign> Barča da ki čyqtylar ḫyšymy YWY-nyŋ boldy<milestone unit="line" n="3"/>
alarġa jamanlyqqa nečiki sözlädi YWY da nečiki ant<milestone unit="line" n="4"/>
etti YWY alarġa da tar boldy alarġa asry.</l>
                    <l n="16" xml:id="Judg-002-016">
                        <foreign xml:lang="he" style="unicode-bidi: embed;">ויקם</foreign> <milestone unit="line" n="5"/> Da turġuzyr edi YWY šaraʿatčylar da qutqaryr edi-<milestone unit="line" n="6"/>
lär alarny qolundan <supplied reason="lost">taluvčylarnyŋ</supplied><ref n="1" target="#Judg-002-fn1" type="noteAnchor"/>.</l>
                <l n="17" xml:id="Judg-002-017">
                    <foreign xml:lang="he" style="unicode-bidi: embed;">וגם</foreign> Da daġyn<milestone unit="line" n="7"/>
šaraʿatčylaryna ešitmäsä edilär ki azdylar<milestone unit="line" n="8"/>
artyndan öŋgä tenrilärniŋ da baš urdular<milestone unit="line" n="9"/>
alarġa azdylar tez ol joldan ki jürüdilär atalary<milestone unit="line" n="10"/>
ešitmä mičvalaryn YWY-nyŋ qylmadylar alaj.</l>
    <l n="18" xml:id="Judg-002-018">
        <foreign xml:lang="he" style="unicode-bidi: embed;">וכי</foreign> Da ki<milestone unit="line" n="11"/>
turġuzyr YWY alarġa šaraʿatčylar da bolyr edi YWY ol<milestone unit="line" n="12"/>
šaraʿatčy bilän da qut<addSpan spanTo="#Judg-002-add001"/>qa<anchor xml:id="Judg-002-add001" type="addSpan"/>ryr edi alarny qolundan dušma-<milestone unit="line" n="13"/>
nlarynyŋ barča künlärindä ol šaraʿatčynyŋ ki<milestone unit="line" n="14"/>
asajyš bolyr edi YWY firjatlaryndan aldyndan qystuvčylarynyŋ<milestone unit="line" n="15"/>
da tar berivčiläriniŋ.</l>
<l n="19" xml:id="Judg-002-019">
    <foreign xml:lang="he" style="unicode-bidi: embed;">והיה</foreign> Da bolġaj ölgenindä<milestone unit="line" n="16"/>
ol šaraʿatčy qajtyr edilär da čajpar edilär atalaryndan<milestone unit="line" n="17"/>
artyq jürümä artyndan öŋgä tenrilärniŋ qulluq<milestone unit="line" n="18"/>
etmä alarġa da baš urma alarġa tüšürmäs edilär<milestone unit="line" n="19"/>
islärin da ol qaty jollaryn.</l>
<l n="20" xml:id="Judg-002-020">
    <foreign xml:lang="he" style="unicode-bidi: embed;">ויחר</foreign> Da qaḥerländi<milestone unit="line" n="20"/>
ačuvu YWY-nyŋ Jisraʾelgä da ajtty anyŋ üčün ki kečtilär<milestone unit="line" n="21"/>
ušbu ulus šol šertimni ki symarladym atalaryna<milestone unit="line" n="22"/>
da ešitmädilär sözimä.</l>
<l n="21" xml:id="Judg-002-021">
    <foreign xml:lang="he" style="unicode-bidi: embed;">גם</foreign> Daġyn men <supplied reason="lost">arttyrmamdyr</supplied>
<ref n="2" target="#Judg-002-fn2" type="noteAnchor"/> sürmä<milestone unit="line" n="23"/>
kišini aldlaryndan ol uluslardan ki kemišti Johošuʿa<milestone unit="line" n="24"/>
da öldi.</l>
<l n="22" xml:id="Judg-002-022">
    <foreign xml:lang="he" style="unicode-bidi: embed;">למען</foreign> Synamaq učun alar bilän šol 
    <supplied reason="lost">Jisraʾelni</supplied><ref n="3" target="#Judg-002-fn3" type="noteAnchor"/>
<milestone unit="line" n="25"/>
saqlarlarmy alar šol jolun YWY-nyŋ jürümä alar bilän nečiki<milestone unit="page" n="37v"/>
<milestone unit="line" n="1"/>
saqladylar atalary jesä joq.</l>
<l n="23" xml:id="Judg-002-023">
    <foreign xml:lang="he" style="unicode-bidi: embed;">וינח</foreign> Da qojdu YWY šol<milestone unit="line" n="2"/>
ušbu uluslarny sürmämäkä alarny tez da bermedi<milestone unit="line" n="3"/>
alarny qoluna Johošuʿanyŋ.</l>
