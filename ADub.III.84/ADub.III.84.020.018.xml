                        <note xml:id="Prov-018-fn2" type="footnote">Heb. <foreign xml:lang="he"
                                style="unicode-bidi: embed;">לפניש־בר</foreign>
                            . Cf. Codex Aleppo: <foreign xml:lang="he" style="unicode-bidi: embed;"
                                >לפני</foreign>
                            -<foreign xml:lang="he" style="unicode-bidi: embed;">שבר</foreign>
                            .</note>
                        <note xml:id="Prov-018-fn3" type="footnote">Heb. <foreign xml:lang="he"
                                style="unicode-bidi: embed;">מדינים</foreign>
                            .</note>
                    </app>
