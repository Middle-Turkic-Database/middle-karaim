                        <note xml:id="fn1" type="footnote">Written erroneously as (<foreign
                                xml:lang="he" style="unicode-bidi: embed;">כי</foreign>
                            ) and corrected to (<foreign xml:lang="he" style="unicode-bidi: embed;"
                                >כא</foreign>
                            )<foreign xml:lang="he" style="unicode-bidi: embed;">שר</foreign>
                            ).</note>
                    </app>
