            <div type="apparatus">
                <listApp>
                    <app>
                        <note xml:id="fn1" type="footnote">Heb. ‘With the help of the God of the
                            Israelites I will write the translation/interpretation of the scroll of
                            the Song of Songs’.</note>
                        <note xml:id="fn2" type="footnote">Heb. incipit of Song 1.1.</note>
                        <note xml:id="fn3" type="footnote">ADub.III.83:
                            <emph>sivirliklerinni</emph>; a scribal error.</note>
                        <note xml:id="fn4" type="footnote">Heb. <foreign xml:lang="he"
                                style="unicode-bidi: embed;">לססתי</foreign>
                            .</note>
                        <note xml:id="fn5" type="footnote">Heb. <foreign xml:lang="he"
                                style="unicode-bidi: embed;">אשכל</foreign>
                            .</note>
                    </app>
                </listApp>
            </div>
